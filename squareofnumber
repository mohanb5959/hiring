pipeline {
    agent any
    parameters {
        string defaultValue: '10', description: 'Enter number', name: 'varOne'
    }
    stages {
        stage('Square of a number') {
            steps {
                script {
                    def varOne = params.varOne as int
                    res = varOne * varOne
                    echo "result is ${res}"
                }
            }
        }
    }
}
